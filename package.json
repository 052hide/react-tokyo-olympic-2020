{
  "name": "react-tokyo-olympic-2020",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@sentry/react": "^6.10.0",
    "@sentry/tracing": "^6.10.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.18",
    "@types/react": "^17.0.36",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-transition-group": "^4.4.2",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "yarn tcm & craco start",
    "build": "craco build",
    "serve": "serve build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix --ignore-path .gitignore",
    "prettier:fix": "prettier --write . --ignore-path .gitignore",
    "generate-client": "openapi-generator-cli generate -g typescript-axios -c ./schema/config.yml -i ./schema/schema.yml -o ./src/generated",
    "generate-mock-server": "openapi-generator-cli generate -g nodejs-express-server -i ./schema/schema.yml -o ./server",
    "tcm": "tcm ./src -w"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.3.9",
    "@sentry/webpack-plugin": "^1.17.0",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-transition-group": "^4.4.2",
    "@types/tailwindcss": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "autoprefixer": "^9.8.6",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-server": "^0.12.3",
    "postcss": "^7.0.35",
    "prettier": "^2.3.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typed-css-modules": "^0.7.0"
  }
}
